    <script type="text/javascript">
        var pathname = window.location.pathname;
        jQuery("ul.nav.nav-list li a").each(function () {
            var href = $(this).attr("href");
            if (pathname == href) {
                $(this).parents("ul").parent("li").addClass("active open");
                $(this).parent("li").addClass("active");
            }
        });

        var contentPath = '@Url.Content("~")'.substr(0, '@Url.Content("~")'.length - 1);
        function ToHomePage() {
            window.open(contentPath + '/Home/Screen1');
        }
        function GetActualityPage() {
            window.open(contentPath + '/Actuality','_parent');
        }
        
        function GetPollutionRealData()
        {
            window.open(contentPath + '/PollutionSource/Actuality', '_parent');
        }
        function GetUsersData()
        {
            window.open(contentPath + '/User', '_parent');
        }
        
        function GetDeviceAnalysis() {
            window.open(contentPath + '/DevStatisticalAnalysis', '_parent');
        }
    </script>
